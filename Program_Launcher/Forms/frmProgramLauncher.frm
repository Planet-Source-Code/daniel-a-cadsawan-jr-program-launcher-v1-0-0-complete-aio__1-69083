Private Sub Set_Sliding()
    On Error Resume Next
    MakeTopMost Me.hwnd
    SlidOut = False
    With Me
        .Left = (Screen.Width - lblMove.Width)
        '.Height = ((Screen.Height / 100) * 90)
        '.Top = ((Screen.Height - .ScaleHeight) / 2)
    End With
    'lblMove.Height = Me.Height
    lblMove.Width = 250
End Sub
Private Sub LblMove_MouseMove(Button As Integer, _
        Shift As Integer, X As Single, Y As Single)
    On Error Resume Next
    'Triggers whether form is currently in or out
    TimOut.Enabled = False
    TimIn.Enabled = False
    'both timers are set to false
    
    If SlidOut = False Then
        TimOut.Enabled = True
        lblMove.BackColor = &HFF00&
        lblMove.ForeColor = &H0&
        'lblMove.Caption = "<"
    Else
        TimIn.Enabled = True
        lblMove.BackColor = &HFF&
        lblMove.ForeColor = &HFFFFFF
        'lblMove.Caption = ">"
    End If
End Sub

Private Sub TimIn_Timer()
    On Error Resume Next
    If Me.Left < (Screen.Width - lblMove.Width) Then
        Me.Left = Me.Left + 100
        'keep moving right until we are in position
    Else
        Me.Left = (Screen.Width - lblMove.Width)
        TimIn.Enabled = False
        SlidOut = False
        'now that it is in, we set SlidOut to false
    End If
End Sub

Private Sub TimOut_Timer()
    On Error Resume Next
    If Me.Left > (Screen.Width - Me.ScaleWidth) Then
        Me.Left = Me.Left - 100
        'keep moving left until we are in position
    Else
        Me.Left = (Screen.Width - Me.ScaleWidth)
        TimOut.Enabled = False
        SlidOut = True
        'now that it is out, we set SlidOut to true
    End If
End Sub

