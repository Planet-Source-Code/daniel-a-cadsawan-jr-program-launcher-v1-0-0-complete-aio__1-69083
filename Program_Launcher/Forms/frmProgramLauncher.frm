Public Sub HideAllStickies()
    Dim frm As Form
    For Each frm In Forms
    iNum = iNum + 1
        If frm.Tag <> "" Then
            'F.Hide
            iNum = iNum + 1
            Unload frm
            Set frm = Nothing
        End If
    Next frm
End Sub

Public Sub UnloadAllForms _
(Optional FormToIgnore As String = "")
    Dim F As Form
    For Each F In Forms
        If F.Name <> FormToIgnore Then
            Unload F
            Set F = Nothing
        End If
    Next F
End Sub

Private Sub UnloadEverything()
    On Error Resume Next
    
    Dim iX As Long
    For iX = 255 To 0 Step -1 '-2
        MakeWindowTransparent Me.hwnd, iX
    Next
    Call RemoveBalloon(Me)
    Call SystrayOff(Me)
    Call UnloadAllForms(frmProgramLauncher.Name)
    Unload frmProgramLauncher
    
End Sub

Private Sub Form_Unload(Cancel As Integer)
On Error Resume Next
    ' unload shell32.dll - used only to turn off using visual styles
    FreeLibrary m_hMod
    Me.Hide
    UnloadEverything
    'ensure unloading proceeds
    Cancel = False
End Sub

