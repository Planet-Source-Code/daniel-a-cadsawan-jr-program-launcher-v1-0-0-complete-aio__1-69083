'Checking The mouse event on my main form in systray
Private Sub Form_MouseMove(Button As Integer, _
Shift As Integer, X As Single, Y As Single)
Static lngMsg As Long
Dim blnflag As Boolean
lngMsg = X / Screen.TwipsPerPixelX

If blnflag = False Then
blnflag = True
        
Select Case lngMsg

Case WM_RBUTTONCLK 'to popup menu on right-click
    Call SetForegroundWindow(Me.hwnd)
    Call RemoveBalloon(Me)
    PopupMenu mnuMenu, , , , mnuAbout 'mnuAbout is bold

Case WM_LBUTTONDBLCLK 'SHow form on left-dblclick
    'Call SetForegroundWindow(Me.hWnd)
    Call SystrayOff(Me)
    Call RemoveBalloon(Me)
    Me.WindowState = vbNormal
    Call mnuShow_Click
        
Case WM_MOUSEMOVE
    Call SetForegroundWindow(Me.hwnd)
    Call SystrayOn(Me, _
    "Program Launcher," & vbCrLf _
    & "Work Scheduler," & vbCrLf _
    & "Sticky Notes and" & vbCrLf _
    & "System Settings" & vbCrLf _
    & "By: Dani Boy!")
        
Case WM_RBUTTONDBLCLK
    Call SetForegroundWindow(Me.hwnd)
    Call RemoveBalloon(Me)
    PopupMenu mnuAdmin, , , , mnuUsage
       
End Select

blnflag = False
        
End If
   
End Sub

