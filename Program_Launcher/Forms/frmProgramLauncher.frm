'CODE : fade form on sctivate
Public Sub Form_Activate()
On Error Resume Next
    Dim sValue As Integer
    sValue = Slider2.Value
    
    'makes the main form static
    Static bActive As Boolean
    If bActive Then Exit Sub
    bActive = True
    
    Dim iX As Long
    For iX = 1 To 255 Step 1 '2
        MakeWindowTransparent frmProgramLauncher.hwnd, iX
        DoEvents ' need this so form doesn't turn black
    Next
    'sets the transparency according to last saved on slider
    SetTrans frmProgramLauncher.hwnd, 255 - sValue * 15
    
    If chkNotes.Value = 1 Then
        cmdShowAllNotes.Enabled = False
        cmdShowAllNotes.Picture = ImgList.ListImages(8).Picture
        Call GetFormsInformation
    
    Else
        mnuShowAllNotes.Enabled = True
        mnuHideAllNotes.Enabled = False
        cmdHideAllNotes.Enabled = False
        cmdHideAllNotes.Picture = ImgList.ListImages(22).Picture
        mnuNewNote.Enabled = False
        cmdCreateNote.Enabled = False
        cmdCreateNote.Picture = ImgList.ListImages(5).Picture
        mnuSaveNotes.Enabled = False
        cmdSaveForms.Enabled = False
        cmdSaveForms.Picture = ImgList.ListImages(11).Picture
        cmdDelAllNotes.Enabled = False
        cmdDelAllNotes.Picture = ImgList.ListImages(23).Picture
        mnuDeleteNotes.Enabled = False
        '    Call GetFormsInformation
        '    mnuHideAllNotes_Click
    End If
    

    If chkClock.Value = 1 Then
        frmClock.show
    End If
        
    'PrLr scheduler form run in background
    frmSched.Hide
    
    Call Display_Icon
    
    If mnuAMini.Checked = True Then Call TimMini_Timing
    
End Sub

Private Sub Form_Initialize()
On Error Resume Next
    m_hMod = LoadLibrary("shell32.dll")
    InitCommonControls
End Sub

Private Sub Form_Load()
On Error Resume Next
    
    Call AppInstance 'in modAppSubs
        
    'CODE : position the form and components
    frmProgramLauncher.Height = 8000
    frmProgramLauncher.Width = 1665
    frmProgramLauncher.Left = _
        (Screen.Width - frmProgramLauncher.Width) - 100
    frmProgramLauncher.Top = _
        (Screen.Height - frmProgramLauncher.Height) - 1000
    lblMove.Top = Me.Height - 1050 - 60
    cmdNotSlide.Top = Me.Height - 1050 - 60
    cmdSlide.Top = Me.Height - 1050 - 60
    Slider2.Top = frmProgramLauncher.Height - 650
    'xFrame1.Expanded = False
    'xFrame2.Expanded = False
    'xFrame3.Expanded = False
    
    'CODE : ERASE the Title Bar
    SetWindowLong Me.hwnd, GWL_STYLE, _
        GetWindowLong(Me.hwnd, GWL_STYLE) + WS_DLGFRAME
    
    'start off transparent so form doesn't flicker
    MakeWindowTransparent Me.hwnd, 1 '10
    
    Call Gradient_Settings 'in modAppSubs
    
    'draw menu bar color
    Draw_MenuBar frmProgramLauncher, _
        (RGB(200, 180, 255)), _
        (RGB(235, 235, 255)), _
        (RGB(235, 235, 255))
    
    'get icon to launch settings registry value
    Call App_Settings
    
    'get slider value in registry
    Slider2.Value = GetSetting(App.EXEName, _
        "Transparency", "Value", Slider2.Value)
    
    'save window position for use by other forms only
    SaveSetting App.EXEName, "WinPos", "PrLrTop", Me.Top
    SaveSetting App.EXEName, "WinPos", "PrLrLeft", Me.Left
    SaveSetting App.EXEName, "WinPos", "PrLrWidth", Me.Width
    SaveSetting App.EXEName, "WinPos", "PrLrHeight", Me.Height
    
    'DATA and Skins Directory Check, create directory if not present
    MakeSureDirectoryPathExists App.Path & "\Data\"
    MakeSureDirectoryPathExists App.Path & "\Skins\"

'name of inifile for sticky notes/app luncher
    inifilename = App.Path & "\Data\" & App.EXEName & "_SNotes.ini"
    inifileapp = App.Path & "\Data\" & App.EXEName & "_App.ini"
    inifilepw = App.Path & "\Data\" & App.EXEName & "_PW.ini"

'checks if the option value is set to load all notes on starting
    chkNotes.Value = GetSetting(App.EXEName, _
        "AutoLoadNotes", "Value", "")
'checks loading speed registry
    hsLoad.Value = GetSetting(App.EXEName, _
        "AutoLoadNotes", "Speed", "")
'checks if the option value is set to yes clock on startup
    chkClock.Value = GetSetting(App.EXEName, _
        "AutoLoadClock", "Value", "")
    'then load clock after activate
'checks tooltip options, get it from registry
    chkToolTip.Value = GetSetting(App.EXEName, _
        "ShowToolTips", "NoShow", "")
'checks automini value in registry
    Dim mini As String
    mini = GetSetting(App.EXEName, _
        "AutoMini", "Value", "")
    mnuAMini.Checked = mini
    hsAMini.Value = GetSetting(App.EXEName, _
        "AutoMini", "Speed", "")

'checks admin lock options, taskbar and taskmanager
    mnuHTask.Enabled = GetSetting(App.EXEName, _
    "AdminLock", "HTaskBar", "")
    mnuSTask.Enabled = GetSetting(App.EXEName, _
    "AdminLock", "STaskBar", "")

    mnuDTMgr.Enabled = GetSetting(App.EXEName, _
    "AdminLock", "HTaskManager", "")
    mnuETMgr.Enabled = GetSetting(App.EXEName, _
    "AdminLock", "STaskManager", "")

    Call Draw_MenuBmp 'in modAppSubs
    
    Dim i As Integer
    For i = 0 To 33
    picApp1(i).Picture = ImgList.ListImages(1).Picture
    Next i
    
    App.TaskVisible = False
    
End Sub

